{
  "name": "Blurry Gmail Fixer",
  "description": "Fix Blurry Icons in Gmail when running 125% display scaling. Fix for the Integrated Layout.",
  "manifest_version": 2,
  "version": "1.2.2",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.gstatic.com/",
    "https://ssl.gstatic.com/",
    "https://mail.google.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{2bcbd966-4dd6-434d-82f0-d1a6fed953ad}"
    }
  }
}